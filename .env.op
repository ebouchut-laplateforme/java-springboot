SPRING_DATASOURCE_URL=jdbc:postgresql://localhost:5433/spring-boot
SPRING_DATASOURCE_USERNAME=op://LaPlateforme/postgres-springboot/username
SPRING_DATASOURCE_PASSWORD=op://LaPlateforme/postgres-springboot/password
